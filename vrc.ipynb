    "def machine_operation(machine_list,x =None,num=None):\n",
    "    # 计算机器的运行天数\n",
    "    def machine_day(machine_list):\n",
    "        for i,j in enumerate(machine_list):\n",
    "            machine_list[i] = list(map(lambda x:x+1,j))\n",
    "        return machine_list\n",
    "    # 移除超过时间的机器\n",
    "    def machine_remove(machine_list):\n",
    "        day_limit = [180,170,160,150]\n",
    "        for i,j in enumerate(machine_list):\n",
    "             machine_list[i] =list(filter(lambda x:x<day_limit[i],j))\n",
    "        return machine_list\n",
    "    # 增加机器个数\n",
    "    def machine_add(machine_list,x,num):\n",
    "        for i in range(num):\n",
    "            machine_list[x].append(0)\n",
    "        return machine_list\n",
    "    # 计算算力\n",
    "    def power_calc(machine_list):\n",
    "        coin_power = len(machine_list[0])*round(32.4/180,2)+len(machine_list[1])*round(175.1/170,2)\n",
    "        +len(machine_list[2])*round(875.2/160,2)+len(machine_list[3])*round(5001/150,2)\n",
    "        return round(coin_power,2)-1.02\n",
    "    \n",
    "    machine_list = machine_day(machine_list)\n",
    "    machine_list = machine_remove(machine_list)\n",
    "    if x is not None:\n",
    "        machine_list = machine_add(machine_list,x,num)\n",
    "    coin_power = power_calc(machine_list)\n",
    "    return machine_list,coin_power"
   ]
